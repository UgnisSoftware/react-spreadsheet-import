  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint",
    "*.{js,ts,tsx,md,html,css,json}": "prettier --write"
  },
  "prettier": {
    "tabWidth": 2,
    "trailingComma": "all",
    "semi": false,
    "printWidth": 120
  }
